: SQR DUP * ;
: CR 10 EMIT ;

: 2DUP OVER OVER ;
: 2DROP DROP DROP ;

: MIN 2DUP < IF DROP ELSE NIP  THEN ;
: MAX 2DUP < IF NIP  ELSE DROP THEN ;

CONSTANT TEN 10

VARIABLE V1

: SUMN
  0
  BEGIN
    OVER +
    SWAP 1 - SWAP
    OVER 0 =
  UNTIL
  NIP ;

: FACTORIAL
  1 2 ROT
  BEGIN
      2DUP <=
  WHILE
      -ROT TUCK
      * SWAP
      1 + ROT
  REPEAT
  2DROP ;

ENTRY

2 SQR 3 SQR MIN . CR
3 SQR 2 SQR MIN . CR

2 SQR 3 SQR MAX . CR
3 SQR 2 SQR MAX . CR

TEN SUMN . CR

4243 V1 !
V1 @ . CR
V1 @ 1 + . CR

7 FACTORIAL . CR

0 HALT

: TICKS 100 IN ;

: SIGN ?DUP IF DUP ABS / ELSE 0 THEN ;

: X!   ( spr -- )   1+  C! ;
: Y!   ( spr -- )   2 + C! ;
: X@   ( n spr -- ) 1+  C@ ;
: Y@   ( n spr -- ) 2 + C@ ;

: PROP! ( n spr -- ) 3 + C! ;
: PROP@ ( n spr -- ) 3 + C@ ;

: STEP ( anim -- )
  DUP     C@ ( INDEX ) 1+
  OVER 1+ C@ ( SIZE  ) %
  SWAP C! ;

: FRAME ( anim -- n )
  DUP C@ ( INDEX ) 2 + + C@ ;

: ANIMATE ( player anim -- )
  DUP STEP FRAME SWAP C! ;

: FACING
  VX @ 0 = IF
    LAST_FACING @
  ELSE
    VX @ SIGN DUP LAST_FACING !
  THEN ;

: LIFT VY @ SIGN ;

: PRESSED? ( key -- )
  PORT_KB OUT
  PORT_KB IN ;

: MOVEMENT-RELEASED
  KEY_RIGHT PRESSED? INVERT
  KEY_LEFT  PRESSED? INVERT
  AND ;

: APPLY-FRICTION
  MOVEMENT-RELEASED IF 
    FACING 0 > IF
      VX @ FRICTION - 0 MAX VX !
    ELSE
      VX @ FRICTION + 0 MIN VX !
    THEN
  THEN ;

: TILE-X TILE_WIDTH  / ;
: TILE-Y TILE_HEIGHT / ;

: TILE ( x y -- adr )
  TILE-Y #TILES_X 2 * * SWAP TILE-X +
  $6300 + ;

: >TYPE ( n -- n )
  DUP 47 = OVER 38 = OR IF
    DROP AIR
    EXIT
  THEN
  DUP 64 = IF DROP SPIKE THEN
  0 OVER 40 BETWEEN?
    OVER 48 = OR
  IF
    DROP SOLID EXIT
  THEN
  DROP AIR ;

: TILE-W  PL_WORLD_X @     PL_WORLD_Y @     TILE C@ >TYPE ;
: TILE-E  PL_WORLD_X @ 7 + PL_WORLD_Y @     TILE C@ >TYPE ;
: TILE-SW PL_WORLD_X @ 1+  PL_WORLD_Y @ 8 + TILE C@ >TYPE ;
: TILE-SE PL_WORLD_X @ 7 + PL_WORLD_Y @ 8 + TILE C@ >TYPE ;
: TILE-NW PL_WORLD_X @ 1+  PL_WORLD_Y @     TILE C@ >TYPE ;
: TILE-NE PL_WORLD_X @ 7 + PL_WORLD_Y @     TILE C@ >TYPE ;

: ON-GROUND?
  TILE-SW SOLID =
  TILE-SE SOLID = OR ;

: CLAMP-VY
  VY @ MAX_FALL > IF
    MAX_FALL VY !
  THEN ;

: APPLY-GRAVITY
  GRAVITY VY +=
  CLAMP-VY ;

: CLAMP-VX
  VX @ MAX_VX > IF MAX_VX VX ! THEN
  VX @ MIN_VX < IF MIN_VX VX ! THEN ;

: ACCELERATE
  KEY_LEFT PRESSED? IF
    VX @ 0 > IF -BRAKING_AX ELSE -AX THEN
    VX +=
    CLAMP-VX
  ELSE
    KEY_RIGHT PRESSED? IF
      VX @ 0 < IF BRAKING_AX ELSE AX THEN
      VX +=
      CLAMP-VX
    THEN
  THEN ;

: START-JUMP
  JUMPING @ IF EXIT THEN
  TRUE JUMPING !
  JUMP_SPEED VY !
  0 JUMP_TIMER ! ;

: BOOST-JUMP
  JUMP_TIMER @ MAX_JUMP <
  JUMPING @ AND IF
    AY VY -=
    JUMP_TIMER ++
  THEN ;

: JUMP
  ON-GROUND? IF
    START-JUMP
  ELSE
    BOOST-JUMP
  THEN ;

: DOOR.ENTRY.X ( door -- n )           @ TILE_WIDTH  * ;
: DOOR.ENTRY.Y ( door -- n )   CELL  + @ TILE_HEIGHT * ;
: DOOR.EXIT.X  ( door -- n ) 2 CELLS + @ TILE_WIDTH  * ;
: DOOR.EXIT.Y  ( door -- n ) 3 CELLS + @ TILE_HEIGHT * ;

: DOOR-ENTRY? ( door -- bool )
  PL_WORLD_X @ OVER DOOR.ENTRY.X PL_WORLD_X @ 8 + BETWEEN?
  PL_WORLD_X @ ROT  DOOR.ENTRY.Y PL_WORLD_Y @ 8 + BETWEEN?
  AND ;

: DOOR-EXIT? ( door -- bool )
  PL_WORLD_X @ OVER DOOR.EXIT.X PL_WORLD_X @ 8 + BETWEEN?
  PL_WORLD_X @ ROT  DOOR.EXIT.Y PL_WORLD_Y @ 8 + BETWEEN?
  AND ;

: DOOR.ENTER   ( door - )
  DUP  DOOR.EXIT.X PL_WORLD_X !
  SWAP DOOR.EXIT.Y PL_WORLD_Y !
  -1 #KEYS += ( TODO ADD -- ) ;

: DOOR.EXIT  ( door - )
  DUP  DOOR.EXIT.X PL_WORLD_X !
  SWAP DOOR.EXIT.Y PL_WORLD_Y !
  -1 #KEYS += ( TODO ADD -- ) ;

: KEYBOARD-INPUT
  KEY_SPACE PRESSED? IF JUMP ELSE FALSE JUMPING ! THEN
  ACCELERATE
  KEY_UP PRESSED? IF
    DOORS C@ 1- FOR
      DOORS 1+ I CELLS + @
      DUP DOOR-ENTRY? #KEYS @ 0 > AND IF
        DOOR.ENTER
      ELSE
        DOOR-EXIT? IF DOOR.EXIT THEN
      THEN
    NEXT
  THEN
  \ PL_WORLD_X @ PL_WORLD_Y @ TILE C@ 47 = AND ( DOOR )
  \ INVENTORY @ 1 = AND IF
  \   28 8 * PL_WORLD_X !
  \   5  8 * PL_WORLD_Y !
  \   46 PL_WORLD_X @ PL_WORLD_Y @ TILE C!    ( OPEN DOOR )
  \   37 PL_WORLD_X @ PL_WORLD_Y @ 1- TILE C! ( OPEN DOOR )
  \   $FE INVENTORY @ AND INVENTORY !
  \ THEN
  KEY_UP PRESSED?
  PL_WORLD_X @ PL_WORLD_Y @ TILE C@ 43 = AND IF
    0 HALT
  THEN ;

: MOVE-X
  VX @ DT @ * PL_WORLD_X_SUB +=
  PL_WORLD_X_SUB @ 8 ARSHIFT ?DUP IF
    PL_WORLD_X +=
    PL_WORLD_X_SUB $FF AND PL_WORLD_X_SUB !
  THEN ;

: MOVE-Y
  VY @ DT @ * PL_WORLD_Y_SUB +=
  PL_WORLD_Y_SUB @ 8 ARSHIFT ?DUP IF
    PL_WORLD_Y +=
    PL_WORLD_Y_SUB $FF AND PL_WORLD_Y_SUB !
  THEN ;

: FINALIZE-X PL_WORLD_X @ CAM_X @ - PLAYER X! ;
: FINALIZE-Y PL_WORLD_Y @ PLAYER Y! ;

: FLIP   PLAYER PROP@ %00000010 OR  PLAYER PROP! ;
: UNFLIP PLAYER PROP@ %11111101 AND PLAYER PROP! ;

: SET-FACING FACING 0 < IF FLIP ELSE UNFLIP THEN ;

: MOVING? VX @ 0 <> VY @ 0 <> OR ;

: SET-ANIMATION
  MOVING? IF RUNNING ELSE IDLE THEN ANIMATION ! ;

: COLLISION-X
  VX @ 0 < IF
    TILE-W SOLID = IF
      PL_WORLD_X @ $FFF8 AND $08 + PL_WORLD_X !
      0 PL_WORLD_X_SUB !
      0 VX !
    THEN
  THEN
  VX @ 0 > IF
    TILE-E SOLID = IF
      PL_WORLD_X @ $FFF8 AND PL_WORLD_X !
      0 PL_WORLD_X_SUB !
      0 VX !
    THEN
  THEN ;

: COLLISION-Y
  VY @ 0 > IF
    TILE-SW SOLID = 
    TILE-SE SOLID = OR IF
      PL_WORLD_Y @ $FFF8 AND PL_WORLD_Y !
      0 VY !
      0 PL_WORLD_Y_SUB !
    THEN
  THEN
  VY @ 0 < IF
    TILE-NW SOLID = 
    TILE-NE SOLID = OR IF
      PL_WORLD_Y @ $FFF8 AND $09 + PL_WORLD_Y !
      0 VY !
      0 PL_WORLD_Y_SUB !
    THEN
  THEN ;

: SHOW ( spr -- ) $01 SWAP PROP! ;

: MOVE-PLAYER
  MOVE-X
  APPLY-FRICTION
  COLLISION-X
  FINALIZE-X
  MOVE-Y
  APPLY-GRAVITY
  COLLISION-Y
  FINALIZE-Y ;

: SCROLL
  PL_WORLD_X @ SCROLL_START_X_RIGHT > IF
    PL_WORLD_X @ SCROLL_START_X_RIGHT -
    0 MAX 256 MIN
    CAM_X !
  THEN
  CAM_X @ PORT_SCROLL OUT ;

ENTRY

$D3F2 PAL 0 CELLS + !
$FF42 PAL 1 CELLS + !

$D3F2 PAL 2 CELLS + !
$FF42 PAL 3 CELLS + !

PLAYER SHOW

BEGIN
  TIMER @ TICKS - ABS DT !

  DT @ 16 > IF   \ 60 FPS
    KEYBOARD-INPUT
    SET-FACING
    MOVE-PLAYER
    SCROLL
    SET-ANIMATION

    PL_WORLD_X @ PL_WORLD_Y @
    VX @ 0 > IF SWAP 8 + SWAP THEN
    2DUP TILE C@ 42 = IF
      63 -ROT TILE C!
    ELSE
      2DUP TILE C@ 45 = IF
        #KEYS ++
        63 -ROT TILE C!
      ELSE
        2DROP
      THEN
    THEN
    
    \ " X=" PRINT PLAYER X@ . "  Y=" PRINT PLAYER Y@ .
    \ "  WX=" PRINT PL_WORLD_X @ . "  WY=" PRINT PL_WORLD_Y @ .
    \ "  CAM_X=" PRINT CAM_X @ . CR

    DEPTH 0 <> IF
      " ERROR: " PRINT DEPTH . CR
    THEN
    

   \ PLAYER Y@ 8 + 224 >= IF 0 HALT THEN
    TICKS TIMER !

  THEN

  ANIM_TIMER @ TICKS - ABS 150 > IF
    PLAYER ANIMATION @ ANIMATE
    TICKS ANIM_TIMER !
  THEN

AGAIN

BYE
